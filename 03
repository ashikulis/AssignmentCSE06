#include <stdio.h>
#include <time.h>

int main()
{
   time_t tm;
   time(&tm);
   printf("Current Date/Time = %s\n\n\n", ctime(&tm));
    int row, col, n, count=0;

    scanf("%d", &n);
    for(row=1; row<=n; row++)
    {
        for(col=1; col<=row; col++)
        {
            printf("%d", ++count);
        }
        printf("\n");
    }
    return 0;
}
